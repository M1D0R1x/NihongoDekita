{
  "builds": [
    {
      "src": "NihongoDekita/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "collectstatic.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "NihongoDekita/wsgi.py"
    }
  ],
  "outputDirectory": "public"
}
